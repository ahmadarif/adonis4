{
  "name": "adonis-fullstack-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "coverage": "nyc npm test",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "Ahmad Arif <ahmad.arif019@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.1",
    "@adonisjs/ally": "^2.0.2",
    "@adonisjs/auth": "^3.0.4",
    "@adonisjs/bodyparser": "^2.0.2",
    "@adonisjs/cors": "^1.0.5",
    "@adonisjs/fold": "^4.0.7",
    "@adonisjs/framework": "^5.0.5",
    "@adonisjs/ignitor": "^2.0.6",
    "@adonisjs/lucid": "^5.0.3",
    "@adonisjs/lucid-slugify": "^1.0.2",
    "@adonisjs/mail": "^3.0.5",
    "@adonisjs/redis": "^2.0.4",
    "@adonisjs/session": "^1.0.23",
    "@adonisjs/shield": "^1.0.6",
    "@adonisjs/validator": "^5.0.3",
    "@adonisjs/vow": "^1.0.15",
    "@adonisjs/websocket": "^1.0.8",
    "adonis-graphql": "^1.0.1-alpha-2",
    "adonis-hashids": "^1.0.4",
    "adonis-kue": "^4.0.0",
    "adonis-scheduler": "^3.0.1",
    "adonis-throttle": "^2.0.1",
    "jaspernode": "^1.1.1",
    "mysql": "^2.15.0",
    "node-whois": "^2.1.3",
    "parse-whois": "^1.0.0",
    "randomstring": "^1.1.5",
    "shortid": "^2.2.8",
    "system-sleep": "^1.3.6"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "nyc": "^11.6.0",
    "pre-commit": "^1.2.2",
    "sqlite3": "^4.0.0",
    "standard": "^11.0.1"
  },
  "autoload": {
    "App": "./app"
  },
  "nyc": {
    "exclude": [
      "test",
      "resources",
      "database",
      "public",
      "tmp",
      "start/socket.js",
      "app/Listeners"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "standard": {
    "globals": [
      "use"
    ],
    "ignore": [
      "public/"
    ]
  }
}
